{
    "name": "farzai/color-palette",
    "description": "A robust PHP library for extracting, analyzing, and managing color palettes from images",
    "keywords": [
        "Farzai",
        "color-palette"
    ],
    "homepage": "https://github.com/parsilver/color-palette-php",
    "license": "MIT",
    "authors": [
        {
            "name": "parsilver",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "nyholm/psr7": "^1.8",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.1",
        "psr/http-message": "^2.0",
        "symfony/http-client": "^6.4"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "mockery/mockery": "^1.6",
        "pestphp/pest": "^2.20",
        "phpstan/phpstan": "^2.0",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Farzai\\ColorPalette\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Farzai\\ColorPalette\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "php -d memory_limit=512M vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint",
        "analyse": "php -d memory_limit=512M vendor/bin/phpstan analyse"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
